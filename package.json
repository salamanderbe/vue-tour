{
  "name": "vue-tour-kiki",
  "description": "A VueJs tour component.",
  "author": "rizkiailham11@gmail.com",
  "license": "MIT",
  "version": "0.0.5",
  "main": "./dist/vue-tour.common.js",
  "keywords": [
    "vue",
    "tour",
    "kiki"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "docs": "vue-cli-service build --dest docs",
    "build-bundle": "vue-cli-service build --target lib --name vue-tour ./src/components/tour.vue",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "vue-template-compiler": "^2.5.17",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rizkiailham/vue-tour"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/rizkiailham/vue-tour/issues"
  },
  "homepage": "https://github.com/rizkiailham/vue-tour#readme",
  "directories": {
    "doc": "docs"
  }
}
